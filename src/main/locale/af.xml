      <items>({see} %c<repeat>, %c</repeat>)</items>
   </list>
   <list name="index-seealsoie">
      <items>({seealso} %c)</items>
      <items>({seealso} %c<repeat>, %c</repeat>)</items>
   </list>
   <list name="glossary-seealso">
      <items>{Seealso} %c.</items>
      <items>{Seealso} %c<repeat>, %c</repeat>.</items>
   </list>
   <list name="varlistentry">
      <items>%c</items>
      <items>%c<repeat>, %c</repeat>
      </items>
   </list>
   <list name="_default">
      <items>%c</items>
      <items>%c {and} %c</items>
      <items>%c<repeat>, %c</repeat>, {and} %c</items>
   </list>
   <group name="glossary">
      <template name="see">{GlossSee} %c.</template>
      <template name="seealso">{GlossSeeAlso} %c.</template>
      <template name="seealso-separator">, </template>
      <template name="glossterm-separator">, </template>
   </group>
   <group name="index"/>
   <group name="msgset">
